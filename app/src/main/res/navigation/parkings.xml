<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parkings"
    app:startDestination="@id/parkingFragment">

    <fragment
        android:id="@+id/parkingFragment"
        android:name="com.example.incidentmanager.ui.fragments.parking.ParkingFragment"
        android:label="fragment_parking"
        tools:layout="@layout/fragment_parking" >
        <action
            android:id="@+id/action_parkingFragment_to_parkingFormFragment"
            app:destination="@id/parkingFormFragment" />
    </fragment>
    <fragment
        android:id="@+id/parkingFormFragment"
        android:name="com.example.incidentmanager.ui.fragments.parking.ParkingFormFragment"
        android:label="fragment_parking_form"
        tools:layout="@layout/fragment_parking_form" >
        <action
            android:id="@+id/action_parkingFormFragment_to_parkingFragment"
            app:destination="@id/parkingFragment" />
    </fragment>
</navigation>